{
  "name": "Betta",
  "version": "1.0",
  "description": "Take care of your own betta fish by being productive!",
  "permissions": ["storage"],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "betta"
  }
}